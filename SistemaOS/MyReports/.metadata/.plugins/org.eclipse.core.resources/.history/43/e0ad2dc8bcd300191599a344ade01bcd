	<queryString language="SQL">
		<![CDATA[select
OSER.os,data_os, status_os,situacao, equipamento,valor,
CLI.nome_Cliente, cel_Cliente, email
from tb_os as OSER
inner join tb_clientes as CLI
on (CLI.id_Cliente = OSER.id_Cliente)]]>
	</queryString>
